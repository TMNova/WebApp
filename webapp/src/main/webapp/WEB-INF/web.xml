<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/webapp_4_0.xsd"
version="4.0">

<welcome-file-list>
    <welcome-file>ServletStart</welcome-file>
</welcome-file-list>

<servlet>
  <servlet-name>ServletStart</servlet-name>
  <servlet-class>ru.lanit.ServletStart</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>ServletStart</servlet-name>
  <url-pattern>/</url-pattern>
</servlet-mapping>

<servlet>
  <servlet-name>ServletName</servlet-name>
  <servlet-class>ru.lanit.ServletName</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>ServletName</servlet-name>
  <url-pattern>/servletname</url-pattern>
</servlet-mapping>

<servlet>
  <servlet-name>ServletSurname</servlet-name>
  <servlet-class>ru.lanit.ServletSurname</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>ServletSurname</servlet-name>
  <url-pattern>/servletsurname</url-pattern>
</servlet-mapping>

<servlet>
  <servlet-name>ServletPatronymic</servlet-name>
  <servlet-class>ru.lanit.ServletPatronymic</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>ServletPatronymic</servlet-name>
  <url-pattern>/servletpatronymic</url-pattern>
</servlet-mapping>

<listener>
  <listener-class>ru.lanit.ContextListener</listener-class>
</listener>

<filter>  
    <filter-name>FirstFilter</filter-name>  
    <filter-class>ru.lanit.FirstFilter</filter-class>  
</filter>  
  
<filter-mapping>  
    <filter-name>FirstFilter</filter-name>  
    <url-pattern>/*</url-pattern>  
</filter-mapping>  

</web-app>
